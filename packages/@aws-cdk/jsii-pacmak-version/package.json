{
  "name": "@aws-cdk/jsii-pacmak-version",
  "description": "A utility to determine what version of jsii-pacmak was this CDK release built with",
  "version": "1.15.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.amazon.awscdk.jsii.pacmak.version",
        "maven": {
          "groupId": "software.amazon.awscdk",
          "artifactId": "cdk-jsii-pacmak-version",
          "versionSuffix": ".DEVPREVIEW"
        }
      },
      "dotnet": {
        "namespace": "Amazon.CDK.JsiiPacmakVersion",
        "packageId": "Amazon.CDK.JsiiPacmakVersion",
        "signAssembly": true,
        "assemblyOriginatorKeyFile": "../../key.snk",
        "versionSuffix": "-devpreview",
        "iconUrl": "https://raw.githubusercontent.com/aws/aws-cdk/master/logo/default-256-dark.png"
      },
      "python": {
        "distName": "aws-cdk.jsii-pacmak-version",
        "module": "aws_cdk.jsii_pacmak_version"
      }
    }
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "pkglint": "pkglint -f",
    "test": "cdk-test",
    "package": "cdk-package",
    "build+test+package": "npm run build+test && npm run package",
    "build+test": "npm run build && npm test"
  },
  "cdk-build": {
    "pre": [
      "./generate.sh"
    ]
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "cdk-build-tools": "^1.15.0",
    "pkglint": "^1.15.0"
  },
  "repository": {
    "url": "https://github.com/aws/aws-cdk.git",
    "type": "git",
    "directory": "packages/@aws-cdk/jsii-pacmak-version"
  },
  "keywords": [
    "aws",
    "cdk"
  ],
  "homepage": "https://github.com/aws/aws-cdk",
  "engines": {
    "node": ">= 10.3.0"
  },
  "stability": "experimental"
}
